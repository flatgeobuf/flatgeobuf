{
  "name": "flatgeobuf",
  "version": "3.32.4",
  "description": "A performant binary encoding for geographic data",
  "scripts": {
    "build": "rm lib -rf && swc ./src/ts -d lib/mjs && mv ./lib/mjs/src/ts/* ./lib/mjs -f && rm ./lib/mjs/src -rf && tsc -p config/tsconfig.mjs.json --emitDeclarationOnly && rollup -c config/rollup.config.js",
    "type-check": "tsc -p config/tsconfig.mjs.json --noEmit",
    "lint": "eslint ./src/ts",
    "test": "vitest ./lib",
    "test-src": "vitest ./src/ts",
    "coverage": "vitest run --coverage",
    "typedoc": "typedoc --tsconfig config/tsconfig.typedoc.json",
    "server": "ws",
    "fmt": "prettier --write src/ts",
    "fmt-check": "prettier --check src/ts"
  },
  "files": [
    "lib/**/*",
    "dist/**/*"
  ],
  "types": "lib/mjs/flatgeobuf.d.ts",
  "module": "lib/mjs/flatgeobuf.js",
  "main": "lib/mjs/flatgeobuf.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flatgeobuf/flatgeobuf.git"
  },
  "keywords": [
    "spatial",
    "geometry",
    "feature",
    "flatbuffers",
    "serialization",
    "binary",
    "performance"
  ],
  "author": "BjÃ¶rn Harrtell",
  "contributors": [
    "Pirmin Kalberer",
    "Michael Kirk",
    "Gabriel Roldan",
    "Jared Erickson",
    "Karsten Pihl",
    "Xavier Fischer",
    "Dustin Carlino",
    "Peter Brodersen",
    "Corey Farwell",
    "Even Rouault",
    "James Hughes",
    "James Gill",
    "Clemens Portele",
    "Horace Williams"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/flatgeobuf/flatgeobuf/issues"
  },
  "homepage": "https://github.com/flatgeobuf/flatgeobuf#readme",
  "dependencies": {
    "@repeaterjs/repeater": "3.0.6",
    "flatbuffers": "24.3.25",
    "slice-source": "0.4.1"
  },
  "optionalDependencies": {
    "ol": ">=3"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.11",
    "@types/geojson": "7946.0.14",
    "@types/node": "22.3.0",
    "@types/stream-buffers": "3.0.7",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "@vitest/coverage-v8": "2.0.5",
    "eslint": "9.9.0",
    "jsts": "2.11.3",
    "lws": "4.1.5",
    "lws-range": "^4.0.1",
    "lws-static": "^3.1.1",
    "node-fetch": "3.3.2",
    "ol": ">=3",
    "prettier": "3.3.3",
    "rollup": "4.20.0",
    "stream-buffers": "3.0.3",
    "typedoc": "0.26.5",
    "typescript": "5.5.4",
    "vitest": "2.0.5",
    "web-streams-polyfill": "4.0.0"
  }
}
