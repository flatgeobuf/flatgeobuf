{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "jsx": false,
            "dynamicImport": false,
            "privateMethod": false,
            "functionBind": false,
            "exportDefaultFrom": false,
            "exportNamespaceFrom": false,
            "decorators": false,
            "decoratorsBeforeExport": false,
            "topLevelAwait": false,
            "importMeta": false
        },
        "transform": null,
        "target": "es2022",
        "loose": false,
        "externalHelpers": false,
        // Requires v1.2.50 or upper and requires target to be es2016 or upper.
        "keepClassNames": false,
        "minify": {
            "compress": {
                "drop_console": true
            },
            "mangle": true,
            "format": {
                "beautify": true
            }
        }
    },
    "minify": true,
    "sourceMaps": true
}
