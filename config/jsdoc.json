{
    "opts": {
        "recurse": true,
        "destination": "dist/doc"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": [],
        "include": ["lib"]
    },
    "sourceType": "module",
    "plugins": ["node_modules/jsdoc/plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "systemName": "FlatGeobuf",
        "copyright": "Bj√∂rn Harrtell",
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true,
            "useLongnameInNav": true
        },
        "applicationName": "FlatGeobuf"
    }
}
