{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "quotemark": [true, "single"],
        "object-literal-sort-keys": false,
        "arrow-parens": [true, "ban-single-arg-parens"],
        "curly": [true, "as-needed"],
        "no-reference": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["**/*_generated.ts"]
    }
}
